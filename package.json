{
    "name": "booking_service_server",
    "version": "1.0.0",
    "description": "test task project",
    "main": "./dist/index.js",
    "scripts": {
        "start": "tsc && nodemon ./dist/index.js",
        "dev": "nodemon ./src/index.ts",
        "build": "tsc",
        "lint": "npx eslint \"{,!(node_modules)/**/}*.{js,ts}\"",
        "lint:staged": "npx eslint",
        "lint:fix": "npm run lint --fix",
        "format": "npm run prettier:fix && npm run lint:fix",
        "prepare": "husky install",
        "prettier": "npx prettier --check .",
        "prettier:fix": "npm run prettier --write \"{,!(node_modules)/**/}*.{js,ts}\""
    },
    "author": "dmtrack",
    "license": "ISC",
    "dependencies": {
        "@types/express": "^4.17.13",
        "bcrypt": "^5.1.1",
        "body-parser": "^1.20.1",
        "cors": "^2.8.5",
        "dotenv": "^16.0.3",
        "express": "^4.18.2",
        "express-mung": "^0.5.1",
        "express-validator": "^6.14.3",
        "husky": "^8.0.3",
        "jsonwebtoken": "^8.5.1",
        "pg": "^8.8.0",
        "pg-hstore": "^2.3.4",
        "prettier": "^3.1.1",
        "reflect-metadata": "^0.1.13",
        "sequelize": "^6.28.0",
        "sequelize-typescript": "^2.1.5",
        "swagger-jsdoc": "^6.2.8",
        "swagger-ui-express": "^5.0.0",
        "zb-sequelize": "^0.0.4"
    },
    "devDependencies": {
        "@commitlint/cli": "^18.4.3",
        "@commitlint/config-conventional": "^18.4.3",
        "@types/bcrypt": "^5.0.0",
        "@types/cors": "^2.8.12",
        "@types/express-mung": "^0.5.2",
        "@types/jsonwebtoken": "^8.5.8",
        "@types/node": "^17.0.31",
        "@typescript-eslint/eslint-plugin": "^5.22.0",
        "@typescript-eslint/parser": "^5.22.0",
        "eslint": "^8.14.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-airbnb-typescript": "^17.0.0",
        "eslint-plugin-import": "^2.26.0",
        "lint-staged": "^15.2.0",
        "nodemon": "^2.0.20",
        "ts-node": "^10.9.1",
        "ts-node-dev": "^1.1.8",
        "typescript": "^4.9.4"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/dmtrack/feather_tasks_server"
